#include <cstdio>

void test(){
    printf("Just test\n");
}

class BigThree{
public:
    BigThree(int test):member(test){
        printf("%d\n", member);
    }

    BigThree(const &BigThree rhs):member(rhs.member) {
        printf("fuck copy %d\n", member);
    }


    BigThree &operator=(const &BigThree rhs){
        member = rhs.member;
        printf("fuck copy %d\n", member);
        return *this;
    }

private:
    int member = 3;
};

void lec_14(){
    /*
     *
     *  error : test (with int type ) shadow test (function)
     *  int test = 3;
     *  test();
     */

    test();
    int test = 3;
    printf("%d\n", test);


    BigThree a(1314);

}
